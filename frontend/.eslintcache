 {\n    const handleProps = () => {\n      if (error) {\n        if (error.msg.title) alert.error('Title is required');\n        if (error.msg.text) alert.error('Text is required');\n        if (error.msg.name) alert.error('Author is required');\n      }\n      if(message) {\n        if(message.postDeleted) alert.success(message.postDeleted);\n        if(message.postAdded) alert.success(message.postAdded);\n      }\n    }\n    handleProps();\n  },[error, message])\n\n  return <Fragment />\n}\n\nAlerts.propTypes = {\n  error: PropTypes.object.isRequired,\n  message: PropTypes.object.isRequired\n}\n\nconst mapStateToProps = state => ({\n  error: state.errors,\n  message: state.messages\n})\n\nexport default connect(mapStateToProps)(withAlert()(Alerts));","/Users/willvanhook/sei/django-blog/frontend/src/reducers/errors.js",[],"/Users/willvanhook/sei/django-blog/frontend/src/reducers/messages.js",[],"/Users/willvanhook/sei/django-blog/frontend/src/actions/messages.js",[],"/Users/willvanhook/sei/django-blog/frontend/src/layouts/Spinner.js",[],{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":1,"column":17,"nodeType":"117","messageId":"118","endLine":1,"endColumn":25},{"ruleId":"115","severity":1,"message":"116","line":1,"column":17,"nodeType":"117","messageId":"118","endLine":1,"endColumn":25},{"ruleId":"119","severity":1,"message":"120","line":17,"column":6,"nodeType":"121","endLine":17,"endColumn":15,"suggestions":"122"},{"ruleId":"123","severity":1,"message":"124","line":23,"column":11,"nodeType":"125","endLine":23,"endColumn":52},{"ruleId":"115","severity":1,"message":"126","line":3,"column":54,"nodeType":"117","messageId":"118","endLine":3,"endColumn":64},{"ruleId":"119","severity":1,"message":"127","line":21,"column":5,"nodeType":"121","endLine":21,"endColumn":21,"suggestions":"128"},"no-native-reassign",["129"],"no-negated-in-lhs",["130"],"no-unused-vars","'useState' is defined but never use